	<FileDownload>
		<!-- The missing rate (in percentage) beyond which file repair is performed by application. -->
		<!-- Value: decimal number from 0.1 to 0.9 or percentage from 10.0 to 90.0 (with % at the end) -->
		<FileRepairThreshold>0.3</FileRepairThreshold>
		<!-- If True, MW requests a complete file -->
		<!--if False, MW abort file repair & remove file (optional) -->
		<FullFileRepair value="false"/>
	</FileDownload>

	<ServiceCoverage>
		<!-- Delay to check MiFi connection. -->
		<WatchdogTimeout>PT4S</WatchdogTimeout>
	</ServiceCoverage>

